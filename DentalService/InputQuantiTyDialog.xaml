<Window x:Class="DentalService.InputQuantiTyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DentalService"
        mc:Ignorable="d"
        Title="InputQuantiTyDialog" Height="126" Width="412">
    <Grid Height="380" VerticalAlignment="Top">
        <Label x:Name="QuantityLabel" Content="Quantity" HorizontalAlignment="Left" Margin="68,23,0,0" VerticalAlignment="Top" RenderTransformOrigin="3.347,1.433"/>
        <TextBox x:Name="QuantityTextBox" HorizontalAlignment="Center" Height="23" Margin="0,23,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SubmitButton" Content="Submit" HorizontalAlignment="Center" Margin="0,70,0,0" VerticalAlignment="Top" Width="76" Click="SubmitButton_Click"/>
    </Grid>
</Window>
